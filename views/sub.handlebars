<table class="table table-hover">
  <tbody>
    <tr>
      <th scope="row" class="table_title">Product</th>
      <td class="table_d"> {{data.product}}</td>
    </tr>
    <tr>
      <th scope="row" class="table_title">Delivery Address</th>
      <td class="table_d">{{data.address}}</td>
    </tr>
    <tr>
      <th scope="row" class="table_title">Start Date</th>
       <td class="table_d">{{data.date}}</td>
    </tr>
    <tr>
      <th scope="row" class="table_title">Duration</th>
       <td class="table_d">{{data.duration}}</td>
    </tr>
    <tr>
      <th scope="row" class="table_title">Amount</th>
       <td class="table_d"> {{data.amount}} â‚¹/-</td>
    </tr>
    <tr>
