<center><h2 class="page-header topone">Account Login</h2></center>

<form method="post" action="/users/login" width="100%" class="login">
  <div class="form-group row one">
    <label  class="col-sm-2 col-form-label">Mobile Number</label>
    <input type="tel" class="form-control col-sm-10" name="username" placeholder="Mobile Number">
  </div>
  <div class="form-group row two">
    <label class="col-sm-2 col-form-label">Password</label>
    <input type="password" class="form-control col-sm-10" name="password" placeholder="Password">
  </div>
  <center><button type="submit" class="btn btnnnn btn-default">Login</button></center>
</form>


